
public class TicTacToe {

	public static void main(String[] args) {
		char[][] grid = new char[3][3];
		
		// populate random and print out
		int position = 1;
		for (int i = 0; i < 3; i++) {
			for (int j = 0; j < 3; j++) {
				grid[i][j] = (char)(position + '0');
				position++;
			}
		}
		
		// display
		for (int i = 0; i < 3; i++) {
			for (int j = 0; j < 3; j++) {
				System.out.print(grid[i][j]);
			}
			System.out.println();
			System.out.println("-----------------");
			System.out.println();
		}
	}

}
